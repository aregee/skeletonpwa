{
  "name": "skeletonpwa",
  "version": "2.2.8",
  "description": "Lightweight vanilla javascript Progressive Webapp framework",
  "main": "bundle/index.js",
  "scripts": {
    "prod": "export NODE_SHELL_ENV=production && ./node_modules/webpack/bin/webpack.js --config webpack.config.js --progress ",
    "serve": "export NODE_SHELL_ENV=production && webpack-dev-server --progress --inline --port 8080 --config webpack.skeleton.config.js",
    "build-sw": "node sw-build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vanilla",
    "pwa",
    "progressive",
    "web",
    "apps",
    "react",
    "angular",
    "vue",
    "mithril"
  ],
  "author": "Rahul Gaur",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.0.1",
    "webpack": "^3.7.1",
    "svg-url-loader": "^2.2.0",
    "webpack-dev-server": "^1.16.5"
  },
  "dependencies": {
    "single-spa": "^3.5.0",
    "single-spa-react": "^2.0.4",
    "universal-router": "6.0.0"
  }
}
